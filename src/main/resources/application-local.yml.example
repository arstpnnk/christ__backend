# Copy this file to `application-local.yml` and fill in your real local secrets.
# Never commit `application-local.yml` (it is ignored by `.gitignore`).
spring:
  datasource:
    url: jdbc:postgresql://localhost:5432/backend_db
    username: postgres
    password: YOUR_DB_PASSWORD

  security:
    oauth2:
      client:
        registration:
          google:
            client-id: YOUR_GOOGLE_CLIENT_ID
            client-secret: YOUR_GOOGLE_CLIENT_SECRET

app:
  jwt:
    # Must be base64-encoded 32+ bytes.
    secret: YOUR_BASE64_JWT_SECRET
